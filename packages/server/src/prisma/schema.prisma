generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String    @id @default(uuid())
  email         String    @unique
  password      String
  createAt      DateTime  @default(now())
}

model Product {
  id            String   @id @default(uuid())
  name          String   @unique
  description   String
  price         Float
  count         Int
  likes         Int
  createAt      DateTime @default(now())
}
